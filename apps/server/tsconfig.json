{
  "compilerOptions": {
    "outDir": "build",
    "module": "commonjs",
    "lib": ["es6"],
    "target": "es2016",
    "strict": true,
    "allowJs": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true
  },
  "include": ["src"]
}
